{
  "name": "@vonojs/framework",
  "version": "0.0.10",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npx tsx build.ts && tsc --declaration --emitDeclarationOnly --project tsconfig.json"
  },
  "dependencies": {
    "@types/node": "^24.7.2",
    "c12": "^3.3.0",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "nitro": "npm:nitro-nightly@latest",
    "typescript": "^5.9.3",
    "vite": "npm:rolldown-vite@7.1.14",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "vono": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/mod.js",
      "types": "./dist/mod.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "types": "./dist/server.d.ts"
    },
    "./client": "./dist/client.d.ts"
  },
  "devDependencies": {
    "esbuild": "^0.25.11"
  }
}
